{
  "cli": {
    "version": ">= 16.1.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "channel": "development"
    },
    "preview": {
      "autoIncrement": true,
      "distribution": "internal",
      "android": {
        "buildType": "app-bundle"
      },
      "channel": "preview",
      "environment": "preview"
    },
    "production": {
      "environment": "production",
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "channel": "production"
    }
  },
  "submit": {
    "alpha": {
      "android": {
        "track": "alpha",
        "serviceAccountKeyPath": "./google-services.json"
      }
    },
    "beta": {
      "android": {
        "track": "beta",
        "serviceAccountKeyPath": "./google-services.json"
      }
    },
    "production": {
      "android": {
        "track": "production",
        "serviceAccountKeyPath": "./google-services.json"
      }
    }
  }
}
